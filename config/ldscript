OUTPUT_FORMAT("elf32-i386")
ENTRY(start)

SECTIONS {
	. = 0x100000;
	
	.setup :
	{
		*(.setup)
	}
	
	. += 0xfc000000;
	
	.text : AT(ADDR(.text) - 0xfc000000)
	{
	        *(.text)
	}
	
	.data ALIGN (4096) : AT(ADDR(.data) - 0xfc000000)
	{
	        *(.data)
	        *(.rodata*)
	}
	
	.bss ALIGN (4096) : AT(ADDR(.bss) - 0xfc000000)
	{
	        *(COMMON*)
	        *(.bss*)
	}
}